lst=[-33,-25,-19,-10,-7,-5,-2,0,3,9,11,17,21,23,25,29,30,33,35,45]
neg=[]
for x in lst:
    if x<0:
        neg.append(x)
print(neg)
neg=[]
for i in range(len(lst)):
    if lst[i]<0:
        neg.append(lst[i])
print(neg)
